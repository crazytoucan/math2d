{
  "name": "math2d",
  "version": "4.0.0",
  "module": "./esm/index.js",
  "main": "./commonjs/index.js",
  "types": "./esm/index.d.ts",
  "scripts": {
    "build": "yarn build:commonjs && yarn build:esm",
    "build:commonjs": "rollup -c rollup.commonjs.js",
    "build:esm": "rollup -c rollup.esm.js",
    "check": "tsc --noEmit",
    "clean": "rm -rf commonjs esm",
    "docs": "scripts/generate-typedoc-data.sh",
    "lint": "tslint --project tsconfig.json",
    "prepack": "yarn clean && yarn build",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@types/jest": "^26.0.19",
    "@wessberg/rollup-plugin-ts": "^1.3.8",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rollup": "^2.35.1",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.1.3",
    "typescript-tslint-plugin": "1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crazytoucan/math2d.git"
  },
  "keywords": [
    "2d",
    "bounding",
    "box",
    "geometry",
    "linear algebra",
    "math",
    "matrix",
    "polygon",
    "polyline",
    "vector"
  ],
  "files": [
    "esm",
    "commonjs"
  ],
  "homepage": "https://crazytoucan.github.io/math2d/",
  "bugs": "https://github.com/crazytoucan/math2d/issues",
  "license": "MIT"
}
